EXEC=./cli4ioc

# GET HYBRID ADDR
# $EXEC --addasset eth.xhy
$EXEC --getaddress eth.xhy |
    while IFS= read -r line
    do
        if [[ "$line" = *"address"* ]]; then
            ADDRESS="${line//Asset\ ETH.XHY\ address\ is:\ /}"
        fi;
    done
